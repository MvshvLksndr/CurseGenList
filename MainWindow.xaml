<Window x:Class="ListWPD.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ListWPD"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <ListView Margin="5" Name="lvContent">
                
            </ListView>
            <Label Margin="5">Страницы</Label>
            <DockPanel x:Name="dpBtns" HorizontalAlignment="Center">
                <Button Margin="5" Name="btnBack" Click="btnBack_Click">Назад</Button>
                <Button Margin="5" Name="btnFrwrd" Click="btnFrwrd_Click">Вперед</Button>
            </DockPanel>
        </StackPanel>
    </Grid>
</Window>
